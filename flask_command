~/dir$ export FLASK_APP=flaskblog.py

~/dir$ export FLASK_DEBUG=1

~/dir$ flask run

//////////// Ou podemos inicializar direto pelo python no shell, sem declarar as variaveis ambiente //////////////
if __name__ == '__main__': # no arquivo flaskblog.py
    app.run(debug=True)

~/dir$ python flaskblog.py
